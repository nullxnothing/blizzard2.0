# Ouroboros Self-Directed Liquidity Bot Configuration

# Required
PRIVATE_KEY=your_base58_private_key_here
TOKEN_MINT=your_token_mint_address_here

# Solana RPC endpoint (use a reliable RPC provider for production)
RPC_URL=https://api.mainnet-beta.solana.com

# Trigger Settings
TRIGGER_THRESHOLD=0.5
GAS_RESERVE=0.02
PRIORITY_FEE=0.005
POLL_INTERVAL=5
SLIPPAGE=50
# Slippage tolerance in percent (50 = 5%)

# Phase 2: Volatility Trading
# How long to hold native token before selling (minutes)
HOLD_DURATION_MINUTES=5

# Phase 3: Diversification Split
# 80% to native token (volatility loop), 20% to trending token (visibility)
SPLIT_RATIO_NATIVE=0.8
SPLIT_RATIO_TRENDING=0.2

# Phase 4: Production
LOG_LEVEL=INFO
DRY_RUN=false
